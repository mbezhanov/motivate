{
  "expo": {
    "name": "Motivate!",
    "slug": "motivate",
    "description": "Mobile app for collecting inspiring quotes, thoughts and ideas.",
    "githubUrl": "https://github.com/mbezhanov/motivate",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
